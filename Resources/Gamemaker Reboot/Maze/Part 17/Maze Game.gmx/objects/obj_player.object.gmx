            <string>pause();
//Movement
if keyboard_check(vk_left) &amp;&amp; place_snapped(32, 32){
    motion_set(180, 4);
    sprite_index = spr_player;
    image_index = 3;
    image_speed = 0;
}
if keyboard_check(vk_up) &amp;&amp; place_snapped(32, 32){
    motion_set(90, 4);
    sprite_index = spr_player;
    image_index = 1;
    image_speed = 0;
}
if keyboard_check(vk_right) &amp;&amp; place_snapped(32, 32){
    motion_set(0, 4);
    sprite_index = spr_player;
    image_index = 2;
    image_speed = 0;
}
if keyboard_check(vk_down) &amp;&amp; place_snapped(32, 32){
    motion_set(270, 4);
    sprite_index = spr_player;
    image_index = 0;
    image_speed = 0;
}
if keyboard_check(vk_nokey) &amp;&amp; place_snapped(32, 32) speed = 0;

//Arrow movement
if place_snapped(32, 32) &amp;&amp; place_meeting(x, y, obj_left) motion_set(180, 8);
if place_snapped(32, 32) &amp;&amp; place_meeting(x, y, obj_right) motion_set(0, 8);
if place_snapped(32, 32) &amp;&amp; place_meeting(x, y, obj_up) motion_set(90, 8);
if place_snapped(32, 32) &amp;&amp; place_meeting(x, y, obj_down) motion_set(270, 8);


//Collisions
if place_meeting(x + hspeed, y + vspeed, obj_wall_corner) &amp;&amp;
!place_meeting(x + hspeed, y + vspeed, obj_block) speed = 0;
//if place_meeting(x + hspeed, y + vspeed, obj_monster_lr) ||
//place_meeting(x + hspeed, y + vspeed, obj_monster_ud){
monster_lrId = instance_place(x + hspeed, y + vspeed, obj_monster_lr);
monster_udId = instance_place(x + hspeed, y + vspeed, obj_monster_ud);
monster_allId = instance_place(x + hspeed, y + vspeed, obj_monster_all);
if monster_lrId != noone ||
monster_udId != noone ||
monster_allId != noone{
    if monster_lrId != noone &amp;&amp; monster_lrId.afraid ||
    monster_udId != noone &amp;&amp; monster_udId.afraid ||
    monster_allId != noone &amp;&amp; monster_allId.afraid{
        audio_play_sound(snd_monster, 0, false);
        score += 50;
        with(monster_lrId){
            x = xstart;
            y = ystart;
            alarm[0] = 1;
        }
        with(monster_udId){
            x = xstart;
            y = ystart;
            alarm[0] = 1;
        }
        with(monster_allId){
            x = xstart;
            y = ystart;
            alarm[0] = 1;
        }
    } else {
        audio_play_sound(snd_dead, 0, false);
        lives -= 1;
        room_restart();    
    }
}
if place_meeting(x + hspeed, y + vspeed, obj_monster_all){
    audio_play_sound(snd_dead, 0, false);
    lives -= 1;
    room_restart();
}
bonusId = instance_place(x + hspeed, y + vspeed, obj_bonus);
if bonusId != noone{
    score += 100;
    audio_play_sound(snd_diamond, 0, false);
    with(bonusId) instance_destroy();
}
lifeId = instance_place(x + hspeed, y + vspeed, obj_life);
if lifeId != noone{
    lives += 1;
    audio_play_sound(snd_life, 0, false);
    with(lifeId) instance_destroy();
}
